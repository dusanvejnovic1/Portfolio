OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
# Available models: gpt-4o-mini, gpt-4o, gpt-5-mini, gpt-5
# GPT-5 models use the Responses API and support optional reasoning.effort
# GPT-4 models use the Chat Completions API
DEFAULT_MODEL=gpt-4o-mini
QUALITY_MODEL=gpt-4o
VISION_MODEL=gpt-4o

# Retrieval APIs for IT Modes
BING_SEARCH_API_KEY=your_bing_search_api_key_here
YOUTUBE_API_KEY=your_youtube_api_key_here

# Google Custom Search Engine (CSE) - Optional
# Get API key from: https://developers.google.com/custom-search/v1/introduction
# Create CSE at: https://cse.google.com/cse/
GOOGLE_CSE_API_KEY=your_google_cse_api_key_here
GOOGLE_CSE_CX=your_google_cse_search_engine_id_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60

# CORS Configuration
ALLOWED_ORIGIN=*